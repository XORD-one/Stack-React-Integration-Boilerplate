{
  "compilerOptions": {
    "baseUrl": "./src",
    "paths": {
      "@components": ["components"],
      "@components/*": ["components/*"],
      "@nativeComponents": ["native components"],
      "@nativeComponents/*": ["native components/*"],
      "@redux": ["redux"],
      "@redux/*": ["redux/*"],
      "@utils": ["utils"],
      "@utils/*": ["utils/*"],
      "@hooks": ["hooks"],
      "@hooks/*": ["hooks/*"],
      "@api": ["api"],
      "@api/*": ["api/*"]
    }
  }
}
